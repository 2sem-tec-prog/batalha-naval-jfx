<VBox alignment="CENTER" prefHeight="484.0" prefWidth="421.0" spacing="20.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.batalhanavaljfx.MenuController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <children>
        <Pane prefHeight="565.0" prefWidth="558.0">
            <children>
                <Button layoutX="129.0" layoutY="318.0" onAction="#onHelloButtonClick" prefHeight="89.0" prefWidth="109.0" text="Jogar">
                    <font>
                        <Font name="Impact" size="30.0" />
                    </font>
                </Button>

                <Label fx:id="welcomeText" layoutX="72.0" layoutY="21.0" prefHeight="62.0" prefWidth="238.0" text="BATALHA NAVAL">
                    <font>
                        <Font name="Impact" size="37.0" />
                    </font>
                </Label>
                <ComboBox fx:id="dificuldade" layoutX="14.0" layoutY="151.0" prefHeight="36.0" prefWidth="180.0" />
                <ComboBox fx:id="tamanho" layoutX="14.0" layoutY="222.0" prefHeight="36.0" prefWidth="180.0" />
                <Text layoutX="14.0" layoutY="139.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Dificuldade">
                    <font>
                        <Font name="Impact" size="18.0" />
                    </font>
                </Text>
                <Text layoutX="14.0" layoutY="212.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Tamanho do tabuleiro">
                    <font>
                        <Font name="Impact" size="18.0" />
                    </font>
                </Text>
            </children>
        </Pane>
    </children>
</VBox>
